/**
 * @version 1.0.0
 * @date Dec 14 2019
 * @param {Property} prop
 * @param {*} value
 * @returns {*}
 */
function as_propSetExprData(prop, value) {
	if ((prop == null) || (value == null)) {
		return false;
	} else {
		prop.expression = "value;//" + JSON.stringify(value);
		return true;
	}
}