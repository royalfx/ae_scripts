/**
 * 
 * @description GET AE RELEASE VERSION
 * @param version {string}
 */
function as_getAeReleaseVersion(version) {
	var verMatch = null;
	for (var n in AEVersion) {
		if (AEVersion.hasOwnProperty(n)) {
			var compare = as_compareVersions(version, AEVersion[n]);
			if (compare == 0) {
				// FOUND
				verMatch = AEVersion[n];
				break;
			}
			else if (compare > 0) {
				// RETURN PREV
				break;
			}
			else {
				// CONTINUE
				verMatch = AEVersion[n];
			}
		} else {
			//
		}
	}
	return verMatch;
}
