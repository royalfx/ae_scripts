/**
 * as_getConstName.jsxinc
 * version: 1.0.2
 * author: Alexander Semeniuk
 * date: Jul 16 2019
 * 
 * @param value 
 * @param constClass 
 */
function as_getConstName(value, constClass) {
    
    for (var p in constClass) {
        if (constClass.hasOwnProperty(p)) {
            if (constClass[p] == value) {
                return p;
            }
        }
    }
    return null;
}
