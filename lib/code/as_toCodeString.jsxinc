 * @param str 
 */
function as_toCodeString(str) {
    str = str.replace(new RegExp("\n", "g"), "\\n");
    str = str.replace(new RegExp("\"", "g"), "\\\"");
    str = str.replace(new RegExp("\t", "g"), "\\t");
    return str;
}
