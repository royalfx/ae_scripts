/**
 * version: 1.0.3
 * author: Alexander Semeniuk
 * date: Mon Feb 05 2018
 * 
 * @param {string} str 
 */
function as_stringClearFirstLastSpaces(str) {
	while (str.indexOf(" ") == 0) {
		str = str.substr(1);
	}
	while (str.substr(-1) == " ") {
		str = str.substr(0, str.length - 1);
	}
	return str;
}
