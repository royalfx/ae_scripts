/**
 * version: 1.0.3
 * author: Alexander Semeniuk
 * date: Mon Feb 05 2018
 * 
 * @description Clear name is indexed
 * @param name {string}
 * @return {string} Result name
 */
function as_nameIndexedClear(name) {
	var splitAt = as_nameIndexedCheck(name);
	if (splitAt > 0) {
		return name.substr(0, splitAt);
	} else {
		return name;
	}
}
