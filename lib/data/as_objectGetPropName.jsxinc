 * @param propValue 
 * @param obj 
 */
function as_objectGetPropName(propValue, obj) {
    for (var p in obj) {
        if (obj.hasOwnProperty(p)) {
            if (obj[p] == propValue) {
                return p;
            }
        }
    }
    return null;
}
