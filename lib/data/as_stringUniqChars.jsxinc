 * 
 * @description MAKE CHARS UNIQUE
 * @param str {string}
 */
function as_stringUniqChars(str) {
	var strUniq = "";
	var len = str.length;
	for (var i = 0; i < len; i++) {
		var c = str.charAt(i);
		if (strUniq.indexOf(c) < 0) {
			strUniq += c;
		}
	}
	return strUniq;
}
